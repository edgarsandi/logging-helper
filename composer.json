{
    "name": "dafiti/logging-helper",
    "description": "Dafiti Logging Helper",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "dafiti",
        "helper",
        "logger",
        "newrelic",
        "logstash"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "monolog/monolog": "~1.17.1"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "fabpot/php-cs-fixer": "@stable",
        "mikey179/vfsStream": "1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Dafiti\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dafiti\\": "tests/"
        }
    }
}